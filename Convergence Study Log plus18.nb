   RowBox[{
    RowBox[{"x1", " ", "=", " ", 
     RowBox[{"3", "/", "8"}]}], ";", 
    RowBox[{"x2", "=", 
     RowBox[{"1", "/", "2"}]}], ";", 
    RowBox[{"x3", "=", 
     RowBox[{"5", "/", "8"}]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"s", "[", "x_", "]"}], ":=", 
    RowBox[{"Piecewise", "[", 
