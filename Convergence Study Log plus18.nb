         RowBox[{"0", "<=", "x", "\[LessEqual]", "x2"}]}], "}"}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"spt2", "[", "x", "]"}], ",", 
         RowBox[{"x2", "<", "x", "\[LessEqual]", "1"}]}], "}"}]}], "}"}], 
     "]"}]}], "\[IndentingNewLine]", 
